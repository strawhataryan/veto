{
  "buildCommand": "npm run build",
  "framework": "vite",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_API_URL": "@veto_api_url",
    "VITE_APP_NAME": "Veto"
  },
  "public": false,
  "git": {
    "deploymentEnabled": true,
    "production": {
      "redeployOnPush": true
    }
  }
}
